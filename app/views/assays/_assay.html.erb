<li class='list-group-item assay'>
  <h4 class="list-group-item-heading"><%= link_to assay.barcode, assay_path(assay.barcode) %></h4>
  <% if assay.has_quant? %>
    <span class="label label-success"><%= link_to assay.quant.name, quant_path(assay.quant) %></span>
  <% else %>
    <span class="label label-default">Unuused</span>
  <% end %>
</li>
