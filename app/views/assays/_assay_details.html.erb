<dl class="summary dl-horizontal">
<dt><%= Assay.human_attribute_name(:assay_set) %></dt>
<dd><%= link_to l(assay.assay_set.created_at, format: :long), assay_set_path(assay.assay_set) %></dd>
<dt><%= Assay.human_attribute_name(:quant) %></dt>
<% if assay.quant? %>
  <dd><%= link_to assay.quant.name, quant_path(assay.quant) %></dd>
<% else %>
  <dd>Unused</dd>
<% end %>

</dl>
